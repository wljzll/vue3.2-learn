#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/node_modules:/mnt/d/project/vue3/node_modules:/mnt/d/project/node_modules:/mnt/d/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/rollup-plugin-typescript2@0.32.1_rollup@2.77.0+typescript@4.7.4/node_modules/rollup-plugin-typescript2/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/rollup-plugin-typescript2@0.32.1_rollup@2.77.0+typescript@4.7.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/bin/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules/resolve/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/resolve@1.22.1/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/node_modules:/mnt/d/project/vue3/node_modules:/mnt/d/project/node_modules:/mnt/d/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/rollup-plugin-typescript2@0.32.1_rollup@2.77.0+typescript@4.7.4/node_modules/rollup-plugin-typescript2/node_modules:/mnt/d/project/vue3/node_modules/.pnpm/rollup-plugin-typescript2@0.32.1_rollup@2.77.0+typescript@4.7.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.1/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.1/node_modules/resolve/bin/resolve" "$@"
fi
